 <p id="notice"><%= notice %></p> 

 <div class="page-header">
  <h1>Itineraries</h1>
  <h6>Activities, Sightseeing, Eating, Drinking, Living. Stay woke.</h6>
</div>

<button type="button" class="btn btn-default">
  <%= link_to "Create a New Itinerary", new_itinerary_path %>
</button>

<% @itineraries.each do |itinerary| %>
<div class ="itinerary">
  Activity Name:<strong>  <%= itinerary.activity_name %></strong>
  <p>Location: <%=itinerary.location %></p>
  <p>Time of Event: <%=itinerary.time_of_event %></p>

<div class = "meta">
  <button type="button" class="btn btn-success">
    <%= link_to "Trip", trips_path %>
  </button> 

  <button type="button" class="btn btn-default">
    <%= link_to "Trip Notes", new_note_path %>
  </button>

  <span class="admin">
    <button type="button" class="btn btn-warning">
      <%= link_to "Edit", edit_itinerary_path(itinerary) %>
    </button>
    <button type="button" class="btn btn-danger">
      <%= link_to "Delete", itinerary, method: :delete, data: { confirm: "Woah! Are you sure you want to delete this itinerary?"} %>
    </button>

  </span><br>
  <%= link_to "Itinerary created " + time_ago_in_words(itinerary.created_at) + " ago", itinerary %>
</div>
</div>
<% end %>





